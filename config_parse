
from easydict import EasyDict as edict
args=edict()

args.dataset='kinect_fruits'
args.net='vgg16_5ch'
args.start_epoch=1
args.max_epochs=30
args.disp_interval=100
args.checkpoint_interval=10000
args.save_dir="/work/jgene/faster_rcnn/data/kinect_fruits_models"
args.num_workers=0
args.cuda=True
args.large_scale=False
args.mGPUs=False
args.batch_size=1 #de normal es 1
args.class_agnostic=False
args.optimizer="sgd"
args.lr=0.0001
args.lr_decay_step=5
args.lr_decay_gamma=0.1
args.session=1
args.resume=False
args.checksession=66
args.checkepoch=1
args.checkpoint=309
args.use_tfboard=False
args.cfg_file="cfgs/vgg16_5ch.yml"
args.set_cfgs=None
args.load_dir="/work/jgene/faster_rcnn/data/kinect_fruits_models"
args.webcam_num=-1
args.image_dir="images_kinect_fruits"
args.parallel_type=0
args.vis=False
args.RGB=True
args.NIR=True
args.DEPTH=True
args.ovthresh=[0.4,0.5,0.6,0.7]
args.minconfid=[0.4,0.5,0.6,0.7]
args.anchor=[2]
args.anchor_ratio=[1.0]

